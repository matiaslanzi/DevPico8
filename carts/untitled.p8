 rectfill(r2.x,r2.y,r2.w,r2.h,3)
	rectfill(r1.x,r1.y,r1.w,r1.h,2)
	
 if(r1.x >= r2.x) then
 	print("x in x")
 end
 
 if(r1.y >= r2.y) then
 	print("y in y")
 end
end
