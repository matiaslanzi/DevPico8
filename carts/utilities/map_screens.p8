__lua__
--map screens
--patreon.com/mboffin

--tab 0:basic game loop
--tab 1:player functions
--tab 2:collision functions
--tab 3:camera/map functions

--only code related to map 
--screens is commented in this 
--cart. grid_movement.p8.png
--has comments for the rest of
--the code.

function _init()

 --set the map tile that will
 --be at the top-left corner of
 --the screen. the map will then
 --be drawn 16 tiles to the left
 --and 16 tiles down from here.
	mx,my=0,0
	
	--this defines  which pixel 
	--coordinates are considered 
	--the top-left of the screen. 
	--usually 0,0 is the top-left,
	--but since the player might
	--now move past the edge of the
	--screen onto other parts of
	--the map, we need a way to
	--keep track of where to place
	--the camera. cx and cy will
	--always be mx*8 and my*8 so
	--that the camera position is 
	--always putting the top-left
	--map tile in the top-left of
	--the screen.
	cx,cy=0,0
	
	make_player()
end

function _update()
	move_player()
	
	--after the player moves, we
	--need to see if we need update
	--mx and my to show a different
	--map screen and move the 
	--camera.
	update_map()
end

function _draw()
	cls()

 --keep the camera matched to
 --the current map screen
	update_camera()
	
	--draw the map starting at mx 
	--and my, and draw 16 tiles 
	--across and 16 tiles down. 
	--because map tiles are
	--8 pixels square, the screen
	--coordinates to draw to are
	--the map coordinates times 8.
	map(mx,my,mx*8,my*8,16,16)
	
	--draw player
	spr(p.sprite,p.x*8,p.y*8)
end
-->8
--player functions

function make_player()
	p={}  --create empty table
	p.x=3 --position on 16x16 map
	p.y=2
	p.sprite=5 --sprite
end

function move_player()

	local new_x,new_y=p.x,p.y

	if (btnp(⬅️)) new_x-=1
	if (btnp(➡️)) new_x+=1
	if (btnp(⬆️)) new_y-=1
	if (btnp(⬇️)) new_y+=1

	if (new_x!=p.x or new_y!=p.y) then	 
	 if (can_move(new_x,new_y)) then
	  p.x=new_x
	  p.y=new_y 
	  sfx(0)
 	end
 end
end
-->8
--collision functions

--this function is the same as
--the can_move() function from
--grid_movement.p8.png, but it's
--condensed into a single line.
function can_move(x,y)
	return fget(mget(x,y))!=1
end
-->8
--camera/map functions

function update_map()
	
	--this will round the player
	--position to the nearest
	--multiple of 16.
	mx=flr(p.x/16)*16
	my=flr(p.y/16)*16
	
end

--update the camera position
--to match the map position.
--this should go in its own
--function, even though it's a
--very simple action, because
--this then allows us to do more
--complex camera changes later
--without having to restructure
--the code.
function update_camera()
 
 --the camera position should
 --be pinned to the top-left
 --map tile represented by 
 --mx and my. it's multiplied by
 --8 because the camera position
 --uses pixel coordinates and
 --map tiles are 8 pixels wide
 --by 8 pixels tall.
 cx=mx*8
 cy=my*8
 
	--this actually changes which
	--pixel coordinates the game
	--draws the screen from.
	--usually pixel 0,0 is the 
	--top-left of the screen, and 
	--then the game draws 128 
	--pixels to the right and 128 
	--pixels down from 0,0. this 
	--just changes which coordinate
	--the game uses at the top-left
	--of the screen, drawing to the
	--right and down from there. 
	camera(cx,cy)
end
