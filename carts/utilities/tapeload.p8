pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
-- 8-bit tape loader
-- abrugsch
-- use and abuse :)
function _init()
frame=0
secs=0
print ("press play on tape")
end
function _update()
frame += 1
secs= flr(frame/30) 
end
function _draw()
 if secs>1 then
  for y=0,123,3 do
   rectfill(0,y,127,y+3,flr(rnd(16)))
  end
  print(secs,1,1) 
  if (secs > 3) then
   rectfill(15,15,112,112,1)
   if(secs%2==0) print("loading...",43,60,8)
  end
 end
end
