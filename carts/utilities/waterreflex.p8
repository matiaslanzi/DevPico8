pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
-- water reflection demo
-- (from the pilot8 game)
--
-- bruno oliveira
-- twitter.com/btco_code


-- pre-generated pseudorandom
-- numbers
local g_rnd=nil
-- global clock, increments 1
-- per frame
local g_clk=0

function _init()
 init_rnd()
end

-- pre-generates pseudorandom
-- numbers for water effect.
function init_rnd()
 g_rnd={}
 for i=1,64 do
  add(g_rnd,flr(rnd(32)))
 end
end

function _update()
 -- clock advances by 1 each
 -- frame
 g_clk+=1
end

-- renders water effect.
-- water_y: y coordinate of
-- water level.
function render_water(water_y)
 -- start at the water line
 -- and copy rows from above
 -- to the rows below to create
 -- the reflection

 -- addr_w is the address we
 -- are writing to.
 local addr_w=0x6000+64*water_y
 -- addr_r is the address we are
 -- reading from.
 local addr_r=addr_w
 -- ripple color (duplicated
 -- because each byte is 2
 -- pixels).
 local rip_c=0x11
 -- draw the horizon.
 memset(addr_w,rip_c,64)
 -- start at the water level
 -- and go down row by row.
 for y=water_y+1,127 do
  -- write address advances by
  -- one row every iteration.
  addr_w+=64
  -- read address goes back
  -- one row every 2 iterations
  -- so that the reflection is
  -- "stretched".
  addr_r-=(y%2)*64
  -- get a pseudo random number
  -- from our pre-generated
  -- array.
  local offset=1+(flr(g_clk/4)+y)%#g_rnd
  local prand=g_rnd[offset]
  -- sometimes draw a reflection,
  -- sometimes draw a ripple.
  -- why band(prand,30)?
  -- no idea. trial and error.
  if 0~=band(prand,30) then
   -- copy the row from above
   -- water into this row
   memcpy(addr_w,addr_r+prand%2*64,64)
  else
   -- draw a fixed-color ripple
   memset(addr_w,rip_c,64)
  end
 end
end

function _draw()
 cls(0)
 -- draw something from the map
 -- so we have something
 -- interesting to reflect.
 map(0,0)
 print("pilot8 water effect",
   band(g_clk,127),88,7)
 -- render the water effect
 -- with the water level at
 -- y=96.
 render_water(96)
end


